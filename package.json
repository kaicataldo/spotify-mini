{
  "name": "spotify-mini",
  "version": "1.0.0",
  "description": "A macOS menubar controller for Spotify!",
  "author": "Kai Cataldo <kaicataldo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && electron dist/main/index.js",
    "dev": "npm run build && NODE_ENV=development nodemon --watch dist/main --exec 'electron dist/main/index.js' & webpack --watch --mode development",
    "lint": "npm run eslint",
    "prettier": "prettier --write '{,!(dist)/**/}*.{js,jsx}'",
    "build": "rimraf dist && webpack --mode production",
    "eslint": "eslint --ext .js,.jsx ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "electron": "^7.2.4",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.0.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.1",
    "postcss-icss-values": "^2.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "open-iconic": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  }
}
