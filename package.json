{
  "name": "spotify-mini",
  "version": "1.0.0",
  "description": "A macOS menubar controller for Spotify!",
  "author": "Kai Cataldo <kaicataldo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && electron dist/main/index.js",
    "dev": "npm run build && nodemon --watch dist/main --exec 'electron dist/main/index.js' & webpack --watch --mode development",
    "lint": "npm run eslint",
    "prettier": "prettier --write '{,!(dist)/**/}*.{js,jsx}'",
    "build": "rimraf dist && webpack --mode production",
    "eslint": "eslint --ext .js,.jsx .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "nodemon": "^1.17.2",
    "postcss-icss-values": "^2.0.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.20.3",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  },
  "dependencies": {
    "normalize.css": "^8.0.0",
    "open-iconic": "^1.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5"
  }
}
