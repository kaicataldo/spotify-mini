{
  "name": "spotify-mini",
  "version": "1.0.0",
  "description": "A macOS menubar controller for Spotify!",
  "author": "Kai Cataldo <kaicataldo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && electron dist/main/index.js",
    "dev": "npm run build && NODE_ENV=development nodemon --watch dist/main --exec 'electron dist/main/index.js' & webpack --watch --mode development",
    "lint": "npm run eslint",
    "prettier": "prettier --write '{,!(dist)/**/}*.{js,jsx}'",
    "build": "rimraf dist && webpack --mode production",
    "eslint": "eslint --ext .js,.jsx ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "electron": "^11.5.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "postcss-icss-values": "^2.0.2",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "open-iconic": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  }
}
