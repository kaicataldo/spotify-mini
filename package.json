{
  "name": "spotify-mini",
  "version": "1.0.0",
  "description": "A macOS menubar controller for Spotify!",
  "scripts": {
    "start": "npm run build -- -p && electron main/index.js",
    "dev": "npm run build && (npm run watch & nodemon --watch main --exec 'electron main/index.js')",
    "test": "npm run lint",
    "build": "rimraf dist && webpack",
    "watch": "webpack --watch src",
    "lint": "eslint --ext .js,.vue .",
    "prettier": "prettier --write '{,!(dist)/**/}*.js'"
  },
  "author": "Kai Cataldo <kaicataldo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.4",
    "electron": "^1.6.11",
    "eslint": "^4.1.1",
    "eslint-plugin-vue": "^3.5.0",
    "nodemon": "^1.11.0",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "prettier": "^1.5.2",
    "rimraf": "^2.6.1",
    "vue-loader": "^13.0.0",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
    "vue": "^2.3.4",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1"
  }
}
