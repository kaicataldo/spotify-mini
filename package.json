{
  "name": "spotify-mini",
  "version": "1.0.0",
  "description": "A macOS menubar controller for Spotify!",
  "scripts": {
    "start-app": "electron main/index.js",
    "build": "rimraf dist && webpack -p",
    "start": "npm run build && npm run start-app",
    "dev": "rimraf dist && webpack && (webpack --watch src & npm run start-app)",
    "lint": "eslint --ext .js,.vue .",
    "prettier": "prettier --write '{,!(dist)/**/}*.js'",
    "test": "npm run lint"
  },
  "author": "Kai Cataldo <kaicataldo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.4",
    "electron": "^1.6.11",
    "eslint": "^4.1.1",
    "eslint-plugin-vue": "^3.5.0",
    "prettier": "^1.5.2",
    "rimraf": "^2.6.1",
    "vue-loader": "^13.0.0",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "vue": "^2.3.4",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1"
  }
}
