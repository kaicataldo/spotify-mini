{
  "name": "spotify-mini",
  "version": "1.0.0",
  "description": "A macOS menubar controller for Spotify!",
  "scripts": {
    "start": "npm run build -- -p && electron dist/main/index.js",
    "dev": "npm run build && (npm run watch & nodemon --watch main --exec 'electron dist/main/index.js')",
    "test": "npm run lint",
    "build": "rimraf dist && webpack",
    "watch": "webpack --watch",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --ext .js,.vue .",
    "stylelint": "stylelint 'src/**/*.vue'",
    "prettier": "prettier --write '{,!(dist)/**/}*.js'"
  },
  "author": "Kai Cataldo <kaicataldo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "electron": "^1.6.11",
    "eslint": "^4.3.0",
    "eslint-plugin-vue": "^3.8.0",
    "nodemon": "^1.11.0",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-html": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
    "open-iconic": "^1.1.1",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1"
  }
}
